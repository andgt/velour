<header class="header container">
    <div class="header__top-menu">
        <div class="header__wrapper">
            {% include '_social.twig' %}
            {% include '_form-search.twig' %}
        </div>
    </div>
    <nav class="main-nav">
        <button class="main-nav__btn-toggle btn" type="button">
            <span class="visually-hidden">Переключатель меню</span>
        </button>
        <ul class="main-nav__list">
            {% for item in nav %}
                <li>
                    <a class="main-nav__link" href="#">{{ item.link }}</a>
                </li>
            {% endfor %}
        </ul>
    </nav>
  <a class="header__logo" href="#">
