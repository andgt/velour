{% extends '../includes/base.twig' %}
{% block title %}
    {{ 'Кольца' }}
{% endblock %}
{% block swiper %}
    ../css/swiper-bundle.min.css
{% endblock %}
{% block favicon %}
    <link rel="icon" href="../favicon.ico">
    <link rel="icon" type="image/png" sizes="32x32" href="../img/favicon-32.png">
{% endblock %}
{% block style %}
    ../css/style.min.css
{% endblock %}

{% block script %}
    ../js/index.min.js
{% endblock %}
{% set mainReturn = 'main.html' %}
{% set logoInnerAddress = '../img/icons/velour.svg' %}
{% block page %}
    {% include '../includes/templates/_header.twig' with logoInnerAddress %}
    {% embed '../includes/catalog-item.twig' %}
        {% block title %} Кольца {% endblock %}
    {% endembed %}
{% endblock %}
